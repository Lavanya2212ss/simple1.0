#!/usr/bin/env groovy

@Library('shared_lib@master') _ //master or whatever branch

pipeline{

      agent any
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                bat ''' 
					
					"C:\\apache-maven-3.8.6\\bin\\mvn" -f %WORKSPACE%/pom.xml clean
					'''
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        filterLogs ('WARNING', 50)
                    }
                }
		
           }	       	     	         
}
